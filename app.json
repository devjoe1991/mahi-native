{
  "expo": {
    "name": "Mahi-nativeapp",
    "slug": "Mahi-nativeapp",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "newArchEnabled": true,
    "privacy": "public",
    "splash": {
      "image": "./assets/splash-icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.mahitechnologies.Mahinativeapp",
      "buildNumber": "1",
      "infoPlist": {
        "NSCalendarsUsageDescription": "Allow Mahi to access your calendar to detect scheduled workouts and help you complete your daily goals.",
        "NSContactsUsageDescription": "Allow Mahi to access your contacts so you can find friends already on Mahi and invite others to join.",
        "NSCameraUsageDescription": "Allow Mahi to use your camera for streak updates and profile photos.",
        "NSPhotoLibraryUsageDescription": "Allow Mahi to access your photos so you can upload media to your profile.",
        "ITSAppUsesNonExemptEncryption": false,
        "NSPrivacyPolicyURL": "https://mahi.fit/privacy-policy"
      },
      "config": {
        "usesNonExemptEncryption": false
      }
    },
    "android": {
      "package": "com.mahitechnologies.Mahinativeapp",
      "versionCode": 1,
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "edgeToEdgeEnabled": true,
      "predictiveBackGestureEnabled": false,
      "permissions": [
        "android.permission.READ_CALENDAR",
        "android.permission.WRITE_CALENDAR",
        "android.permission.READ_CONTACTS",
        "android.permission.CAMERA",
        "android.permission.READ_MEDIA_IMAGES",
        "android.permission.READ_EXTERNAL_STORAGE"
      ]
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "plugins": [
      "expo-font",
      [
        "expo-calendar",
        {
          "calendarPermission": "Allow Mahi to access your calendar to detect scheduled workouts and help you complete your daily goals."
        }
      ],
      [
        "expo-contacts",
        {
          "contactsPermission": "Allow Mahi to access your contacts so you can find friends already on Mahi and invite others to join."
        }
      ]
    ],
    "extra": {
      "links": {
        "privacyPolicy": "https://mahi.fit/privacy-policy",
        "support": "https://mahi.fit/support",
        "terms": "https://mahi.fit/terms"
      },
      "eas": {
        "projectId": "64a44e79-9439-45bb-bd01-f10fcb19f86c"
      }
    }
  }
}
